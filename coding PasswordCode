package selfProgramming;
import java.util.Scanner; 
public class PasswordCode {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String Username;
		String Password;
		
		Password = "102001";
		Username = "Dunia";
		
		Scanner varinput1 = new Scanner (System.in);
		System.out.println("PLEASE ENTER YOUR USERNAME");
		String username = varinput1.next();
		
		Scanner varinput2 = new Scanner (System.in);
		System.out.println("PLEASE ENTER YOUR PASSWORD");
		String password = varinput2.next();
		
		if (username.equals(Username)&& password.equals(Password)) {
			System.out.print("ACCESS GRATED ! WELCOME YASRI");
			
		}
		
		else if (username.equals(Username)) {
			System.out.print("INVALID PASSWORD");
		}
		
		else if (password.equals(password)) {
			System.out.print("INVALID USERNAME");
		
		}else {
			System.out.print("INVALID USERNAME AND PASSWORD!!");
		}
				
		
	}
	
}
