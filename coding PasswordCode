package selfProgramming;
import java.util.Scanner;
public class Passwordcodetest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner (System.in);
		
		String Email;
		String Password;
		
		Email = "duniatipu@gmail.com.my";
		Password = "123456";
		
		System.out.println("ENETER YOUR EMAIL");
		String email = sc.next();
		
		System.out.println("ENTER YOUR PASSWORD");
		String password = sc.next();
		
		
		if (email.equals(email)&& password.equals(Password)) {
				System.out.print("ACCESS GRATED ! WELCOME YASRI");
		}
		
		else if (email.equals(password)) {
			System.out.print("INVALID EMAIL");
		}
		
		else if (password.equals(email)) {
			System.out.print("INVALID PASSWORD");
		}
		
		else {
			System.out.print("INVALID USERNAME AND PASSWORD!!");
		}
		
		
		
		
		
	}

}
